{
    "vim.enableNeovim": false,
    "files.hotExit": "off",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "workbench.editor.enablePreview": false,
    "omnisharp.loggingLevel": "error",
    "omnisharp.projectLoadTimeout": 9999999,
    "vsicons.projectDetection.autoReload": true,
    "diffEditor.ignoreTrimWhitespace": true,    
    "gitlens.gitExplorer.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.avatars": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.recentChanges.highlight.locations": [],
    "tslint.alwaysShowStatus": true,
    "tslint.alwaysShowRuleFailuresAsWarnings": true,
    "window.newWindowDimensions": "maximized",
    "editor.lineNumbers": "relative",
    "window.zoomLevel": 0,
    "workbench.sideBar.location": "left",
    "problems.autoReveal": false,
    "csharp.suppressHiddenDiagnostics": false,
    "gitlens.keymap": "none",
    "vsicons.dontShowNewVersionMessage": true,
    "omnisharp.autoStart": true,
    "vim.leader": "<space>",   
    "window.menuBarVisibility": "default",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
    "vim.hlsearch": true,
    "vim.statusBarColorControl": false,
    "vim.visualstar": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "workbench.statusBar.visible": true,    
    "workbench.activityBar.visible": true,
    "csharp.format.enable": false,
    "csharpfixformat.style.braces.onSameLine": false,
    "csharpfixformat.style.newline.elseCatch": true,
    "gitlens.historyExplorer.enabled": false,
    "typescript.preferences.quoteStyle": "single",
    "terminal.integrated.rendererType": "canvas",
    "debug.toolBarLocation": "docked",    
    "git.confirmSync": false,
    "git.enableSmartCommit": true,    
    "terminal.integrated.fontSize": 11,
    "editor.snippetSuggestions": "none",
    "typescript.tsc.autoDetect": "off",
    "mssql.resultsFontFamily": "",
    "vim.easymotionMarkerFontFamily": "Input",
    "csharp.referencesCodeLens.enabled": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "line-thin",
    "smoothtype.duration": 300,
    "smoothtype.autoReload": true,
    "breadcrumbs.enabled": true,
    "npm.autoDetect": "off",
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "window.titleBarStyle": "native",
    "git.autofetch": true,
    "gitlens.views.repositories.enabled": false,
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "C_Cpp.clang_format_style": "Google",
    "gitlens.mode.active": "zen",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "[cpp]": {},
    "editor.tabSize": 2,
    "editor.detectIndentation": false
}
